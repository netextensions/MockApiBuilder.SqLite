<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>NetExtensions</RootNamespace>
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>NetExtensions MockApiBuilder.SqLite</Title>
    <Description>MockApiBuilder.SqLite</Description>
    <PackageProjectUrl>https://github.com/netextensions/MockApiBuilder.SqLite</PackageProjectUrl>
    <RepositoryUrl>https://github.com/netextensions/MockApiBuilder.SqLite</RepositoryUrl>
    <PackageTags>MockApiBuilder, mock api,.net core</PackageTags>
    <PackageId>NetExtensions.MockApiBuilder.SqLite</PackageId>
    <PackageVersion>1.0.1</PackageVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Authors>netextensions.io</Authors>
    <PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="icon.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="7.0.0" />
    <PackageReference Include="MediatR.Extensions.Microsoft.DependencyInjection" Version="8.0.0" />
    <PackageReference Include="NetExtensions.Extensions.SqLite" Version="1.0.1" />
    <PackageReference Include="NetExtensions.ServiceHostBuilder" Version="1.0.0" /> 
    <PackageReference Include="NetExtensions.Extensions.SwashbuckleWithOauth" Version="1.0.0" />
    <PackageReference Include="NetExtensions.Extensions.SerilogApplicationBuilder" Version="1.0.0" />
    <PackageReference Include="NetExtensions.Core.Handlers" Version="1.0.4" />
  </ItemGroup>

</Project>
